    "from IPython.display import IFrame\n",
    "from IPython.core.display import display\n",
    "\n",
    "lat = '32.050'\n",
    "lon = '118.767'\n",
    "google_map_url = \"http://maps.google.com/maps?q={0}+{1}&ie=UTF8&t=h&z=14&{0},{1}&output=embed\".format(lat, lon)\n",
    "display(IFrame(google_map_url, '425px', '350px'))"
